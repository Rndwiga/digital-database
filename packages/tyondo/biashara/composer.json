{
    "name": "tyondo/biashara",
    "description": "An e-commerce package for laravel",
    "keywords": ["hardware", "laravel","ecommerce","biashara"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.6.4",
        "laravel/framework": "5.3.*|5.4.*",
        "laravelcollective/html": "5.3.*|5.4.*"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7"
    },
    "autoload": {
        "classmap": [
            "database"
        ],
        "psr-4": {
	         "Tyondo\\Biashara\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "suggest": {
        "tyondo/mnara": "Simple user management package for laravel"
    },
    "minimum-stability": "stable",
    "scripts": {
        "post-install-cmd": [
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan optimize"
        ]
    }
}
